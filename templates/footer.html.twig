                <li>
                    {{ '<a href="@sendinblue-feature" target="_blank">> Features</a>'|t({ '@sendinblue-feature': 'https://www.sendinblue.com/features/?utm_source=drupal_plugin&utm_medium=plugin&utm_campaign=module_link' }) }}
