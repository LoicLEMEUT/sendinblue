                    {{ '<a href="@sendinblue-tutorial" target="_blank">> Features</a>'|t({ '@sendinblue-tutorial': 'https://resources.sendinblue.com/category/tutorials/?utm_source=drupal_plugin&utm_medium=plugin&utm_campaign=module_link' }) }}
