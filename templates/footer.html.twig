<div id="wrap-right-side" class="box-border-box  col-md-3">
    <div class="panel panel-default text-left box-border-box  small-content">
        <div class="panel-heading"><strong>{% trans %}About SendinBlue{% endtrans %}</strong></div>
        <div class="panel-body"><p>
                {% trans %}SendinBlue is an online software that allows you to send emails and SMS. Easily manage
                your
                Marketing campaigns, transactional emails and SMS.{% endtrans %}
            </p>
            <ul class="sib-widget-menu">
                <li>
                    {{ '<a href="@sendinblue-about" target="_blank">> Who we are</a>'|t({ '@sendinblue-about': 'https://www.sendinblue.com/about/?utm_source=drupal_plugin&utm_medium=plugin&utm_campaign=module_link' }) }}
                </li>
                <li>
                    {{ '<a href="@sendinblue-price" target="_blank">> Pricing</a>'|t({ '@sendinblue-price': 'https://www.sendinblue.com/pricing/?utm_source=drupal_plugin&utm_medium=plugin&utm_campaign=module_link' }) }}
