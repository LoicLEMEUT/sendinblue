                    {{ '<a href="@sendinblue-faq" target="_blank">> FAQ</a>'|t({ '@sendinblue-faq': 'https://resources.sendinblue.com/category/faq/?utm_source=drupal_plugin&utm_medium=plugin&utm_campaign=module_link' }) }}
